# Sets the number of skill points (Ex: Fighter has 2+Int).
:DetermineSkillCount
# Fighter
1,|SkillCount=2|
# Barbarian
2,|SkillCount=4|
# Bard
3,|SkillCount=6|
# Cleric
4,|SkillCount=2|
# Druid, Monk
5-6,|SkillCount=4|
# Paladin
7,|SkillCount=2|
# Ranger
8,|SkillCount=6|
# Rogue
9,|SkillCount=8|
# Sorcerer, Wizard
10-11,|SkillCount=2|

:SetAbilityBonus
1,|STRBonus={If~%STR%>=10?{Trunc~{Calc~(%STR%-10)/2}}/{Floor~{Calc~(%STR%-10)/2}}}|
_|DEXBonus={If~%DEX%>=10?{Trunc~{Calc~(%DEX%-10)/2}}/{Floor~{Calc~(%DEX%-10)/2}}}|
_|CONBonus={If~%CON%>=10?{Trunc~{Calc~(%CON%-10)/2}}/{Floor~{Calc~(%CON%-10)/2}}}|
_|INTBonus={If~%INT%>=10?{Trunc~{Calc~(%INT%-10)/2}}/{Floor~{Calc~(%INT%-10)/2}}}|
_|WISBonus={If~%WIS%>=10?{Trunc~{Calc~(%WIS%-10)/2}}/{Floor~{Calc~(%WIS%-10)/2}}}|
_|CHABonus={If~%CHA%>=10?{Trunc~{Calc~(%CHA%-10)/2}}/{Floor~{Calc~(%CHA%-10)/2}}}|

# Temp1 and Temp2 each calculates a minimum value of 1.
:CalculateTotalSkillPoints
1,|Temp={Calc~(%SkillCount%+%INTBonus%)}|{If~%Temp%<1?|Temp=1|}
#_|TotalSkillPoints={Calc~(((%SkillCount%+%INTBonus%)*4)+((%SkillCount%+%INTBonus%)*(%CharacterLevel%-1)))}|
_|TotalSkillPoints={Calc~((%Temp%*4)+(%Temp%*(%CharacterLevel%-1)))}|
_{If~%Race%=1?|TotalSkillPoints={Calc~(%TotalSkillPoints%+(4+(%CharacterLevel%-1)))}|}
_|MaxRanks={Calc~(%CharacterLevel%+3)}|
_|CrossClassRanks={Trunc~{Calc~(%MaxRanks% / 2)}}|
_|SkillCountAtMaxRanks={Ceil~{Calc~(%TotalSkillPoints% / %MaxRanks%)}}|

