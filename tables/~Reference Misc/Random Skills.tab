@Race,8,Choose a Race,Human,Dwarf,Elf,Gnome,Half Elf,Half Orc,Halfling,Random
@Class,11,Choose a Class,Fighter,Barbarian,Bard,Cleric,Druid,Monk,Paladin,Ranger,Rogue,Sorcerer,Wizard,Random
@CharacterLevel,99,Enter the Character Level.
@FamiliarID,11,Choose Familiar,Bat,Cat,Hawk,Lizard,Owl,Rat,Raven,Snake,Toad,Weasel,Random,None
@STR,99,Input Strength
@DEX,99,Input Dexterity
@CON,99,Input Constitution
@INT,10,Input Intelligence
@WIS,99,Input Wisdom
@CHA,99,Input Charisma

:Start
1,{If~{Param~Race,%Race%}=Random?|Race={Dice~1d7}|}
_{If~{Param~Class,%Class%}=Random?|Class={Dice~1d11}|}
_{If~%CharacterLevel%=99?|CharacterLevel={Dice~1d20}|}
_{If~{Param~FamiliarID,%FamiliarID%}=Random?|FamiliarID={Dice~1d10}|}
_{If~%STR%=99?|STR={Dice~3d6}|}{If~%DEX%=99?|DEX={Dice~3d6}|}
_{If~%CON%=99?|CON={Dice~3d6}|}{If~%INT%=99?|INT={Dice~3d6}|}
_{If~%WIS%=99?|WIS={Dice~3d6}|}{If~%CHA%=99?|CHA={Dice~3d6}|}
_[GenerateSkills]

:GenerateSkills
1,[DetermineSkillCount=%Class%][SetAbilityBonus][CalculateTotalSkillPoints]
# Familiar Updater
_{If~{And~{Param~Class,%Class%}=Sorcerer,{Param~FamiliarID,%FamiliarID%}!=None}=1?[FamiliarSkillsAdjustments=%FamiliarID%]}
_{If~{And~{Param~Class,%Class%}=Wizard,{Param~FamiliarID,%FamiliarID%}!=None}=1?[FamiliarSkillsAdjustments=%FamiliarID%]}
_
_|LanguageString=[SetLanguages=%Race%]|
_{If~%INTBonus%>0?{Loop~%INTBonus%,[LoopLanguage]}}
_
_|SkillsString=|
_|SkillCount=%SkillCountAtMaxRanks%|
_
_{While~%SkillCount%,
_|Skill=[ClassSkillsBranch]|
_|SkillsString&{If~%CommaFlag%=1?;}%Skill%|
_|CommaFlag=1||SkillCount-1|
_}
_
_|LanguageString={OrderAsc~";",%LanguageString%}|
_|LanguageString={Replace~;,; ,%LanguageString%}|
_|LanguageString={CommaReplace~1,;,%LanguageString%}|
_
_|SkillsString={OrderAsc~";",%SkillsString%}|
#_|Pos={Find~1,%SkillString%,","}|
#_|SkillsString={Right~{Calc~{Length~%SkillsString%}-%Pos%-1},%SkillsString%}|
_|SkillsString={Replace~;,; ,%SkillsString%}|
_|SkillsString={CommaReplace~1,;,%SkillsString%}|
# CapEachWord should be replaced with TitleCap when available.
_%SkillsString%!%TotalSkillPoints%!{If~%FamSkillID%=0?0/+%FamSkillBonus% [SkillsList=%FamSkillID%]}!{CapEachWord~%LanguageString%}
#_[TestTable]

# -----------------------------------------------------------------------
# To test this table, activate [TestTable] above by removing the #
# and put a # in the beginning of the line above [TestTable]

:TestTable
1,<b>Race:</b> {Param~Race,%Race%}<br>
_<b>Class:</b> {Param~Class,%Class%} %CharacterLevel%<br><br>
_<b>Familiar:</b> {Param~FamiliarID,%FamiliarID%}<br>
_<b>Familiar Skill Bonus:</b> {If~%FamSkillID%=0?None/+%FamSkillBonus% [SkillsList=%FamSkillID%]}<br><br>
_<b>Stats:</b> Str %STR% (%STRBonus%), Dex %DEX% (%DEXBonus%), Con %CON% (%CONBonus%), Int %INT% (%INTBonus%), Wis %WIS% (%WISBonus%), Cha %CHA% (%CHABonus%)<br><br>
_<b>Skills: (%SkillCountAtMaxRanks% skills / %TotalSkillPoints% SP)</b> %SkillsString%<br><br>
# CapEachWord should be replaced with TitleCap when available.
_<b>Languages:</b> {CapEachWord~%LanguageString%}

