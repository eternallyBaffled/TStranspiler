TableSmith52 {

  Table
    = ListOf<group, eol>

  group
    = groupheader eol groupbody

  groupheader
    = ":" name
    | ";" name

  name  (a name)
    = "start"
    | alnum+

  groupbody
    = nonemptyListOf<groupline, eol>

  groupline
    = number "," template

  number
    = digit+

  template
    = templatechar+

  templatechar
    = ~eol any

  eol
    = "\r"? "\n"
}
